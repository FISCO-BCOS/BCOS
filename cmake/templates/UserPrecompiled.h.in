#include "libblockverifier/ExecutiveContextFactory.h"
@USER_PRECOMPILED_HEADER@


    void
    dev::blockverifier::ExecutiveContextFactory::registerUserPrecompiled(
        std::shared_ptr<tbb::concurrent_unordered_map<Address,
            std::shared_ptr<precompiled::Precompiled>, std::hash<Address>>>
            _buildInPrecompiled)
{
    // Address should in [0x5001,0xffff]
    _buildInPrecompiled->insert(std::make_pair(dev::precompiled::DAG_TRANSFER_ADDRESS,
        std::make_shared<dev::precompiled::DagTransferPrecompiled>()));
    _buildInPrecompiled->insert(std::make_pair(dev::precompiled::PAILLIER_ADDRESS,
        std::make_shared<dev::precompiled::PaillierPrecompiled>()));
    _buildInPrecompiled->insert(std::make_pair(dev::precompiled::GROUP_SIG_ADDRESS,
        std::make_shared<dev::precompiled::GroupSigPrecompiled>()));
    _buildInPrecompiled->insert(std::make_pair(dev::precompiled::RING_SIG_ADDRESS,
        std::make_shared<dev::precompiled::RingSigPrecompiled>()));
}
